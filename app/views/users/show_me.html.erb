

<div class="col-md-4">
  <div class="jumbotron">
    <h1>My Profile</h1>
    <br>
    <p>
      <strong>Name:</strong>
      <%= @user.name %>
    </p>
    
    <p>
      <strong>Email:</strong>
      <%= @user.email %>
    </p>
    
    <p>
      <strong>Courses: </strong>
      <%if @user.courses.length >=1%>
            <%= "| " %>
            <% @user.courses.each do |course| %>
              <%= course.name + " | " %>
            <% end %>
      <% end %>
    </p>
    
    <p>
      <strong>Score:</strong>
      <%= @user_score %>
    </p>
  </div>
  </div>
