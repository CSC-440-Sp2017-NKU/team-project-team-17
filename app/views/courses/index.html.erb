<p id="notice"><%= notice %></p>

<h1>Courses</h1>

<div class="container">
  <div class="row">
    <div class="col-md-4">
    
    <% @courses.each do |course| %>
      <div class="card">
        <div class="card-block">
          <h4 class="card-title"><%= course.name %></h4>
          <h6 class="card-subtitle mb-2 text-muted">Taught by: <%= course.professor %></h6>
          <p class="card-text"><%= course.description %></p>
          <a class="card-link"><%= link_to 'View Questions', course %></a>
        </div>
      </div>
    <% end %>
   
     <div class="col-md-4">
       <h1>Stuff</h1>
     </div>
   
   </div>
  </div>
</div>
<br>

<%= link_to 'New Course', new_course_path %>
